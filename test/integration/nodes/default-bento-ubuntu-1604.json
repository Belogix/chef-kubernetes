{
  "id": "default-bento-ubuntu-1604",
  "chef_environment": "_default",
  "automatic": {
    "roles": ["etcd"],
    "network": {
      "interfaces": {
        "enp0s3": {
          "addresses": {
            "10.0.2.15": {
              "family": "inet"
            }
          }
        }
      }
    },
    "platform": "ubuntu",
    "fqdn": "default-bento-ubuntu-1604.vagrantup.com"
  },
  "default": {
    "kubernetes": {
      "interface": "enp0s3"
    }
  },
  "run_list": [
    "role[etcd]",
    "role[kubernetes_master]",
    "role[kubernetes_node]"
  ]
}
