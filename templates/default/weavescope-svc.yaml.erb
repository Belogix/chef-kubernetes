apiVersion: v1
kind: Service
metadata:
  name: weavescope-app
  namespace: kube-system
  labels:
    app: weavescope
    kubernetes.io/cluster-service: "true"
spec:
  ports:
  - name: app
    port: 4040
    targetPort: 4040
    protocol: TCP
  selector:
    app: weavescope
    weavescope-component: weavescope-app
  type: ClusterIP
